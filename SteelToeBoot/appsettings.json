{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    }
  },
  "spring": {
    "application": {
      "name": "steeltoeboot"
    },
    "eureka": {
      "client": {
        "serviceUrl": "http://localhost:8761/eureka/",
        "shouldFetchRegistry": false,
        "ValidateCertificates": false
      },
      "instance": {
        "port": 5000
      }
    },
    "cloud": {
      "config": {
        "failfast": true,
        "retry": {
          "enabled": true,
          "maxInterval": 10000,
          "maxAttempts": 1000
        },
        "discovery": {
          "enabled": true,
          "serviceId": "config"
        }
      }
    }
  }
}
